!= form_for(resource, :as => resource_name, :url => create_person_session_path) do |f|
  .section
    %h2 Sign In
    %fieldset
      %p
        != f.label :email
        != f.text_field :email
      %p
        != f.label :password
        != f.password_field :password
  %p!= f.submit 'Sign in'
!= render :partial => 'devise/shared/links'
